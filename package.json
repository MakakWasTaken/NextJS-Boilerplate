{
  "name": "koerselsliste",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build && yarn postbuild",
    "postbuild": "yarn build:sitemap & yarn copy:static & yarn copy:public",
    "build:sitemap": "next-sitemap",
    "copy:public": "copyfiles './public/**/*' ./build/standalone/",
    "copy:static": "copyfiles -u 1 '!**/standalone/**' './build/**/*' ./build/standalone/build",
    "start": "node build/standalone/server.js",
    "lint": "biome format --write --no-errors-on-unmatched . & biome lint --apply --no-errors-on-unmatched .",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky install",
    "typescript:check": "tsc --noEmit",
    "prisma:update": "prisma format && prisma generate && prisma db push",
    "test": "yarn test:jest && yarn test:playwright",
    "test:jest": "jest",
    "test:playwright": "npx playwright test"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.2.0",
    "@babel/core": "^7.23.7",
    "@date-io/core": "^3.0.0",
    "@date-io/luxon": "^3.0.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@mui/base": "^5.0.0-beta.34",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "@mui/system": "^5.15.7",
    "@mui/x-data-grid-pro": "^6.19.3",
    "@mui/x-date-pickers-pro": "^6.19.3",
    "@mui/x-license-pro": "^6.10.2",
    "@next/bundle-analyzer": "^14.1.0",
    "@next/mdx": "^14.1.0",
    "@prisma/client": "^5.9.1",
    "accept-language": "^3.0.18",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cookies": "^0.9.1",
    "cross-env": "^7.0.3",
    "green-analytics-js": "^1.1.14",
    "i18next": "^23.8.2",
    "lru-cache": "^10.2.0",
    "luxon": "^3.4.4",
    "next": "^14.1.0",
    "next-auth": "^4.24.5",
    "next-i18next": "^15.2.0",
    "next-seo": "^6.4.0",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^6.9.9",
    "playwright": "^1.41.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.1",
    "sharp": "^0.33.2",
    "sonner": "^1.4.0",
    "swr": "^2.2.4",
    "uuid": "^9.0.1",
    "webpack": "^5.90.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.2",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@playwright/test": "^1.41.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookies": "^0.9.0",
    "@types/copyfiles": "^2",
    "@types/luxon": "^3.4.2",
    "@types/mdx": "^2.0.11",
    "@types/node": "^20.11.16",
    "@types/nodemailer": "^6",
    "@types/react": "^18.2.51",
    "@types/uuid": "^9.0.8",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.1",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.1",
    "prisma": "^5.9.1",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@4.0.2"
}
